{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES5", // TSはECMAScript 5に変換
    "module": "ES2015", // TSのモジュールはES Modulesとして出力
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "assets/*": [
        "./assets/*"
      ],
      "src/*": [
        "./src/*"
      ]
    },
  }
}